;Field: SYMBOLS & MOTIFS
RECTANGLE=,<frame,TS, 0.05>,#000000, #000000
FONT=<TITLE_FRONT>
TEXT=,"SYMBOLS & MOTIFS",<frame,TS,0.5>,left,wordwrap
FONT=<CONTENT_FRONT>
TEXT="1-{(SYMBOLS & MOTIFS)}",[SYMBOLS & MOTIFS],<frame,TS,0.5>,left,wordwrap
;Field: STORY OPPORTUNITIES
RECTANGLE=,<frame,TS, 0.05>,#000000, #000000
FONT=<TITLE_FRONT>
